<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <BorderPane prefHeight="108.0" prefWidth="363.0">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <bottom>
            <Button mnemonicParsing="false" onAction="#acceptPath" text="Accept" BorderPane.alignment="CENTER">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <BorderPane.margin>
                  <Insets bottom="20.0" left="280.0" right="10.0" top="25.0" />
               </BorderPane.margin>
               <padding>
                  <Insets bottom="7.0" left="17.0" right="17.0" top="7.0" />
               </padding>
            </Button>
         </bottom>
         <top>
            <Label text="Pleace enter the path of your catalog with music:" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets left="-60.0" right="30.0" top="20.0" />
               </BorderPane.margin>
            </Label>
         </top>
         <center>
            <TextField fx:id="pathField" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </BorderPane.margin>
            </TextField>
         </center>
         <GridPane.margin>
            <Insets />
         </GridPane.margin>
      </BorderPane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
